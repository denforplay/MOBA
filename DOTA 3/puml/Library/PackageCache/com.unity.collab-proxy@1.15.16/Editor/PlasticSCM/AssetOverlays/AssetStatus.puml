@startuml
enum AssetStatus {
    None= 0,
    Private= 1 << 0,
    Ignored= 1 << 2,
    Added= 1 << 3,
    Checkout= 1 << 4,
    Controlled= 1 << 5,
    UpToDate= 1 << 6,
    OutOfDate= 1 << 7,
    Conflicted= 1 << 8,
    DeletedOnServer= 1 << 9,
    Locked= 1 << 10,
    LockedRemote= 1 << 11,
    HiddenChanged= 1 << 12,
}
class LockStatusData {
}
class ClassifyAssetStatus {
    {static} ContainsAny(status:AssetStatus, matchTo:AssetStatus) : bool
}
@enduml
