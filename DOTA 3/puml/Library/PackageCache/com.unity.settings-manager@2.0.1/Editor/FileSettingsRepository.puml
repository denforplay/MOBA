@startuml
class FileSettingsRepository {
    <<const>> k_PrettyPrintJson : bool = true
    m_Initialized : bool
    m_Path : string
    + FileSettingsRepository(path:string)
    Init() : void
    + path : string <<get>>
    + name : string <<get>>
    + TryLoadSavedJson(json:string) : bool
    + Save() : void
    + Set(key:string, value:T) : void
    + Get(key:string, fallback:T) : T
